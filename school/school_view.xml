        <record id="school_view_form" model="ir.ui.view">
            <field name="name">subject</field>
            <field name="model">school.student</field>
            <field name="arch" type="xml">
                <!--<link rel="stylesheet" href="/school/static/src/css/school.css"/>-->
                <form string="subject">
                    <sheet>
                        <group>
                            <field name="name" string="Student"/>
                            <field name="grade_class" string="Grade/Class"/>
                            <field name="school_year" string="School year"/>

                         </group>

                        <separator name="toinvoice" string="Score"/>
                        <field name="sujects_ids" widget="one2many_list" nolabel="1">
                              <tree editable="bottom" >
                                  <field name="term" string="Select Term"/>
                                  <field name="sub_english" string="English"/>
                                  <field name="sub_social_student" string="Social Student"/>
                                  <field name="sub_math" string="Math"/>
                                  <field name="sub_science" string="Science"/>
                                  <field name="sub_french" string="French"/>
                                  <field name="sub_arabic" string="Arabic"/>
                                  <field name="sub_art" string="Art"/>
                                  <field name="sub_pe" string="PE"/>
                                  <field name="sub_music" string="Music"/>
                                  <field name="sub_computer" string="Computer"/>
                                  <field name="sub_religion" string="Religion"/>
                              </tree>
                          </field>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="subject_action_menu_item" model="ir.actions.act_window">
            <field name="name">Subject</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">school.student</field>
            <field name="view_mode">tree,form</field>

        </record>



        <menuitem id="menu_item_subject" name="Subject"  />

        <menuitem id="menu_item_subject_sub" name="Suject" parent="menu_item_subject" />

        <menuitem id="menu_item_subject_sub_action" name="Subject for Student" parent="menu_item_subject_sub" action="subject_action_menu_item" />
    </data>
</openerp>
